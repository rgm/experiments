{
  "src/*.cljc": {
    "alternate": "test/{}_test.cljc",
    "type": "source"
  },
  "test/*_test.cljc": {
    "alternate": "src/{}.cljc",
    "type": "test"
  },
  "src/*.cljs": {
    "alternate": "test/{}_test.cljs",
    "type": "source"
  },
  "test/*_test.cljs": {
    "alternate": "src/{}.cljs",
    "type": "test"
  },
  "src/*.clj": {
    "alternate": "test/{}_test.clj",
    "type": "source"
  },
  "test/*_test.clj": {
    "alternate": "src/{}.clj",
    "type": "test"
  },
  "*": {"make": "make"}
}
